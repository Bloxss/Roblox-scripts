local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Bloxss/Roblox-scripts/main/epic%20leaked%20paid%20ui%20lib%20go%20do%20it%20by%20your%20self"))()
local Window = lib:Window("Title Example - Synapse X","Version Example")
local Tab = Window:Tab("Tab")
local Tab2 = Window:Tab("Tab 2")

Tab:Button("Button Example", function()
    print("This button has been pressed")
 end)
 
 Tab:Button("Notification Example", function()
    lib:Notify("Notification", "Notification")
 end)

 Tab:Toggle("Toggle Example", function(value)
    print(value)
 end)

 Tab:Slider("Slider Example",5 , 100, 50, 1, function(value)
print(value)
end)

Tab:Dropdown("Dropdown Example","Select one",{"1","2","3","4","5"}, function(value)
